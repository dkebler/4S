{
  "build": {
    "type": "dev",
    "localdevport": "8090"
  },
  "deploy": {
    "gh-pages": {
      "location": "default",
      "default": {
        "url": "dkebler.github.io/super-static-site-starter"
      },
      "test": {
        "url": "dkebler.github.io/test",
        "repo": "https://github.com/dkebler/test.git"
      }
    },
    "local": {
      "path": "",
      "serverPort": "8080"
    },
    "s3": {
      "location": "testing",
      "testing": {
        "profile": "DGK-deploy",
        "region": "us-west-2",
        "url": "test.david.kebler.net",
        "bucket": "test.david.kebler.net"
      },
      "live": {
        "profile": "DGK-deploy",
        "region": "us-west-2",
        "url": "david.kebler.net",
        "bucket": "david.kebler.net"
      }
    }
  },
  "dir": {
    "build": "builds",
    "buildSub": {
      "dev": "dev/",
      "dist": "dist/"
    },
    "lib": "lib",
    "assets": "assets",
    "content": "content"
  },
  "fonts": {
    "base": "Times-Roman"
  },
  "html": {
    "template": "hugo",
    "generator": "hugo"
  },
  "images": {}
}
